<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="500.0" stylesheets="@holidayWindow.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="diagrep.gui.HolidayWindowController">
   <children>
      <Label layoutX="148.0" layoutY="23.0" styleClass="header" text="Manage Holidays" />
      <ListView fx:id="list" layoutX="35.0" layoutY="133.0" prefHeight="337.0" prefWidth="228.0" />
      <Label layoutX="35.0" layoutY="99.0" styleClass="bold" text="Saved Holidays" />
      <DatePicker fx:id="dateP" layoutX="309.0" layoutY="133.0" prefHeight="31.0" prefWidth="148.0" />
      <Button fx:id="addButton" layoutX="309.0" layoutY="317.0" mnemonicParsing="false" onAction="#addHoliday" prefHeight="65.0" prefWidth="148.0" styleClass="buttonUI" text="ADD" />
      <Button fx:id="deleteButton" layoutX="309.0" layoutY="405.0" mnemonicParsing="false" onAction="#deleteHoliday" prefHeight="65.0" prefWidth="148.0" styleClass="buttonUI" text="DELETE" />
   </children>
</AnchorPane>
